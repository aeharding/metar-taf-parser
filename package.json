{
  "name": "metar-taf-parser",
  "version": "3.1.2",
  "description": "Parse METAR and TAF reports",
  "homepage": "https://aeharding.github.io/metar-taf-parser",
  "keywords": [
    "aviation",
    "weather",
    "metar",
    "taf",
    "report",
    "deserialize",
    "decoder"
  ],
  "main": "dist/index.js",
  "author": "Alexander Harding <2166114+aeharding@users.noreply.github.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/aeharding/metar-taf-parser.git"
  },
  "scripts": {
    "build": "rollup -c rollup.config.ts",
    "clean": "rimraf dist",
    "start": "rollup -c rollup.config.ts -w",
    "check-types": "tsc --noEmit",
    "check-formatting": "prettier --check '**/*.{js,json,css,md,scss,tsx,ts}'",
    "watch": "watch 'yarn build' src",
    "test": "jest --coverage",
    "test-watch": "jest --watch --coverage",
    "prepublishOnly": "yarn clean && yarn build"
  },
  "files": [
    "dist",
    "assets"
  ],
  "devDependencies": {
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@types/jest": "^27.4.1",
    "gzip-size-cli": "^5.1.0",
    "jest": "^27.5.1",
    "lodash": "^4.17.21",
    "prettier": "^2.6.2",
    "rollup": "^2.71.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-dts": "^4.2.1",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-typescript-paths": "^1.3.1",
    "rollup-plugin-typescript2": "^0.31.2",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.2.1",
    "tsc-alias": "^1.6.6",
    "typescript": "^4.4.2",
    "watch": "^1.0.2"
  }
}
